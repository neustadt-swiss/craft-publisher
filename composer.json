{
  "name": "nst/craft-publisher",
  "description": "Publisher X enables you to publish saved Drafts on a future date without the need to handle the cache expiration logic. The cronjob handles the publication and the cache invalidation.",
  "version": "5.0.0",
  "license": "MIT",
  "type": "craft-plugin",
  "require": {
    "craftcms/cms": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "neustadt\\publisher\\": "src/"
    }
  },
  "extra": {
    "handle": "publisher-x",
    "name": "Publisher X",
    "schemaVersion": "2.0.6",
    "developer": "Neustadt",
    "developerUrl": "http://neustadt.ch",
    "class": "neustadt\\publisher\\Publisher"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
